[alias]
	all = add -A
	amend = commit --amend -C HEAD
	ci = commit
	co = checkout
	count = !~/.dotfiles/git/count
	credit = !~/.dotfiles/git/credit
	nuke = !~/.dotfiles/git/nuke
	st = status -sb
    stat = diff --stat
[color]
	ui = auto
[core]
	editor = gedit
	excludesfile = ~/.ignoregit
[push]
	default = simple
[user]
	email = EMAIL
	name = NAME
